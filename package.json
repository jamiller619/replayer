{
  "name": "replayer",
  "version": "1.0.0",
  "main": "src/main/main.js",
  "author": "Jeff Miller",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "concurrently --kill-others --success first --raw \"npm run web-dev\" \"npm run app-dev\"",
    "web-dev": "parcel ./src/main/index.html -d dist/dev",
    "web": "rm -f dist/dev/* && parcel ./src/main/index.html -d dist/dev",
    "app-dev": "ELECTRON_START_URL=http://localhost:1234 electron ."
  },
  "browserslist": [
    "last 1 Chrome versions"
  ],
  "dependencies": {
    "@electron/remote": "^1.0.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "electron": "^11.2.2",
    "electron-store": "^7.0.1",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.2.1"
  }
}
